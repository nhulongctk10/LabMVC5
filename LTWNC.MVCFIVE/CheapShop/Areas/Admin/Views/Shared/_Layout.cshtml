<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @Styles.Render("~/Content/admbootstrap")
    @RenderSection("pluginStyles", false)
    @Styles.Render("~/Content/admstyle")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="wrapper">
        @Html.Partial("_NavigationPartial")

        <div id="page-wrapper" class="gray-bg">
            <div class="row border-bottom">
                @Html.Partial("_NavBarHeaderPartial")
            </div>

            <div class="row wrapper border-bottom white-bg page-heading">
                @Html.Partial("_PageHeaderPartial")
            </div>

            <div class="wrapper wrapper-content">
                @RenderBody()
            </div>

            <footer>
                @Html.Partial("_FooterPartial")
            </footer>
        </div>
    </div>

    @Scripts.Render("~/bundles/admmainjs")
    @RenderSection("pluginScripts",false)
    @Scripts.Render("~/bundles/admcustomjs")
    @RenderSection("scripts", required: false)
</body>
</html>